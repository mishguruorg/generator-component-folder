import React from 'react'
import test from 'ava'
import { mount } from 'enzyme'

<% if (isComp) { -%>
import <%= name %> from './'
<% } else { -%>
import { <%= name %> } from './'
<% } -%>

const baseProps = {}

test('<<%= name %>/>', t => {
  const wrapper = mount(
    <<%= name %> {...baseProps} />
  )
  t.truthy(wrapper.find('<%= elm %>'))
})
